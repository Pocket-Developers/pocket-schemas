{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.getpocket.io/schema/Program.json",
  "description": "Program",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "Program ID"
    },
    "program_type": {
      "type": "string",
      "$ref": "https://schemas.getpocket.io/schema/ProgramType.json"
    },
    "start_date": {
      "type": "string",
      "format": "date",
      "title": "Date program was started"
    }
  },
  "required": [
    "id",
    "program_type",
    "start_date"
  ],
  "additionalProperties": false
}