{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.getpocket.io/schema/MilestoneCompletion.json",
  "description": "MilestoneCompletion",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "id of credential"
    },
    "milestone_id": {
      "type": "string",
      "title": "id of milestone associated with completion"
    },
    "milestone_name": {
      "type": "string",
      "title": "name of milestone type"
    },
    "recognition_date": {
      "type": "string",
      "format": "date",
      "title": "date recognized"
    },
    "term": {
      "$ref": "https://schemas.getpocket.io/schema/Term.json"
    }
  },
  "required": [
    "id",
    "milestone_id",
    "milestone_name",
    "recognition_date",
    "term"
  ],
  "additionalProperties": false
}