{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.getpocket.io/schema/Graduate.json",
  "description": "Graduate",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "id of Graduate credential"
    },
    "name": {
      "type": "string",
      "title": "name of Graduate"
    },
    "description": {
      "type": "string",
      "title": "description of Graduate credential",
      "examples": [
        "A graduate or alumnus of an institution.."
      ]
    },
    "graduation_date": {
      "type": "string",
      "format": "date",
      "title": "date of graduation"
    },
    "programs": {
      "type": "array",
      "title": "ids for the programs completed, if any",
      "items": {
        "type": "string"
      }
    },
    "related_credentials": {
      "type": "array",
      "title": "ids of related degree or certificate credential(s)",
      "items": {
        "type": "string"
      }
    },
    "comments": {
      "type": "string",
      "title": "Any additional comments added upon Graduate"
    }
  },
  "required": [
    "id",
    "gpa",
    "letter_grade",
    "program_gpa",
    "program_letter_grade"
  ],
  "additionalProperties": false
}