{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.getpocket.io/schema/StudentAffiliation.json",
  "description": "StudentAffiliation",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "Credential Id"
    },
    "organization_id": {
      "type": "string",
      "title": "id for the organization / institution"
    },
    "organization_name": {
      "type": "string",
      "title": "Name of the organization / institution"
    },
    "level_id": {
      "type": "string",
      "title": "class level ID"
    },
    "level_name": {
      "type": "string",
      "title": "Class level name"
    },
    "enrollment_date": {
      "type": "string",
      "format": "date",
      "title": "Date accepted/enrolled as student"
    }
  },
  "required": [
    "id",
    "organization_id",
    "organization_name",
    "level_id",
    "level_name",
    "enrollment_date"
  ],
  "additionalProperties": false
}