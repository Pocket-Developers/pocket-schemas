{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.getpocket.io/schema/Student.json",
  "description": "Student",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "id of Student Credential"
    },
    "name": {
      "type": "string",
      "title": "name of Student Credential"
    },
    "description": {
      "type": "string",
      "title": "description of Student Credential"
    },
    "acceptance_date": {
      "type": "string",
      "format": "date",
      "title": "date of acceptance"
    },
    "student_type_id": {
      "type": "string",
      "title": "id of Student type"
    },
    "comments": {
      "type": "string",
      "title": "Any additional comments added upon Student"
    }
  },
  "required": [
    "id",
    "name",
    "description",
    "acceptance_date",
    "student_type_id"
  ],
  "additionalProperties": false
}