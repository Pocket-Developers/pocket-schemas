{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.getpocket.io/schema/ClassWithdrawal.json",
  "description": "ClassWithdrawal",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "id of credential"
    },
    "course": {
      "$ref": "https://schemas.getpocket.io/schema/Course.json"
    },
    "class_id": {
      "type": "string",
      "title": "id of enrolled class"
    },
    "withdrawal_date": {
      "type": "string",
      "format": "date",
      "title": "withdrawal date"
    },
    "term": {
      "$ref": "https://schemas.getpocket.io/schema/Term.json"
    },
    "grade": {
      "$ref": "https://schemas.getpocket.io/schema/Grade.json"
    }
  },
  "required": [
    "id",
    "course",
    "class_id",
    "incomplete_date",
    "term",
    "grade"
  ],
  "additionalProperties": false
}