{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.getpocket.io/schema/ClassCompletion.json",
  "description": "Class Completion",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "Class Completion ID"
    },
    "class": {
      "type": "string",
      "$ref": "https://schemas.getpocket.io/schema/Class.json"
    },
    "grade": {
      "type": "string",
      "$ref": "https://schemas.getpocket.io/schema/Grade.json"
    },
    "completion_date": {
      "type": "string",
      "format": "date",
      "title": "Date class was completed"
    }
  },
  "required": [
    "id",
    "class",
    "grade",
    "completion_date"
  ],
  "additionalProperties": false
}