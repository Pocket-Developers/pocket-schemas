{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.getpocket.io/schema/PriorProgram.json",
  "description": "Prior Program",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "Credential id"
    },
    "program_id": {
      "type": "string",
      "title": "id of program record"
    },
    "name": {
      "type": "string",
      "title": "Organization name for program"
    },
    "description": {
      "type": "string",
      "title": "Description of the program"
    },
    "abbreviation": {
      "type": "string",
      "title": "Abbreviation of program, if any"
    },
    "orgqanization_id": {
      "type": "string",
      "title": "Organization offering the program, if any"
    },
    "external_id": {
      "type": "string",
      "title": "Organization's id of program, if any"
    },
    "program_type_id": {
      "type": "string",
      "title": "id of program type"
    },
    "program_map_id": {
      "type": "string",
      "title": "id of course map for program completion, if any"
    },
    "completion_definition_id": {
      "type": "string",
      "title": "Completion credential definition the program offers, if any"
    },
    "reference_uri": {
      "type": "string",
      "title": "uri of the program's web page, if any"
    }
  },
  "required": [
    "id",
    "program_id",
    "name",
    "program_type"
  ],
  "additionalProperties": false
}