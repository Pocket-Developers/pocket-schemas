{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.getpocket.io/schema/CollageAffiliation.json",
  "description": "CollageAffiliation",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "Credential Id"
    },
    "college_id": {
      "type": "string",
      "title": "name of college"
    },
    "college_name": {
      "type": "string",
      "title": "Program Id on Institution"
    },
    "parent_id": {
      "type": "string",
      "title": "id of parent organization"
    },
    "parent_name": {
      "type": "string",
      "title": "Name of parent organization, if any"
    },
    "date": {
      "type": "string",
      "format": "date",
      "title": "date enrolled"
    }
  },
  "required": [
    "id",
    "college_id",
    "college_name",
    "date"
  ],
  "additionalProperties": false
}